{
  "expo": {
    "name": "Confluency",
    "slug": "language-tutor",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/transparent_background_icon.png",
    "userInterfaceStyle": "light",
    "scheme": "confluency",
    "description": "Interactive language learning app with AI-powered tutoring. By using this app, you agree to our Terms of Service (https://persistent-lychee-e09.notion.site/Confluency-Terms-of-Service-EULA-1f2282c115ae804ba5aec1f6239b6962?pvs=4) and Privacy Policy (https://persistent-lychee-e09.notion.site/Confluency-Privacy-Policy-1e4282c115ae80ed8f27ce8c8cfb2e9e?pvs=4).",
    "android": {
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "confluency"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#4A6FA5"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "io.github.kvgharbigit.langlearn",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSMicrophoneUsageDescription": "This app uses the microphone to record your speech for language practice.",
        "UIBackgroundModes": ["audio"]
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/transparent_background_icon.png",
        "backgroundColor": "#4A6FA5"
      },
      "package": "io.github.kvgharbigit.langlearn",
      "permissions": ["RECORD_AUDIO", "MODIFY_AUDIO_SETTINGS"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "newArchEnabled": true,
    "plugins": [
      "expo-web-browser"
    ],
    "extra": {
      "eas": {
        "projectId": "08897675-6d90-4552-ab1f-b81ecea27c1a"
      },
      "supabaseUrl": "https://hkanndmudvnjrvkisklp.supabase.co",
